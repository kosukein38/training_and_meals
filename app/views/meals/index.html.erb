<div class="m-8 w-full">
  <h1 class="text-xl font-bold"><%= t '.title' %></h1>
  <%=link_to workouts_path, class: "btn btn-outline m-4" do %>
    <button><%= (t '.to_workouts_page') %></button>
  <% end %>
  <div class="m-8 flex flex-wrap w-full">
    <% if @meals.present? %>
      <%= render @meals %>
    <% else %>
      <p><%= t('.no_result') %></p>
    <% end %>
  </div>
</div>
